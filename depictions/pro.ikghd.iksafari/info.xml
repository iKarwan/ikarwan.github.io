<package>
	<id>pro.ikghd.iksafari</id>
	<name>iKsafari</name>
	<version>1.0-1</version>
	<compatibility>
		<firmware>
            <miniOS>11.0</miniOS>
			<maxiOS>13.7</maxiOS>
			<otherVersions>unconfirmed</otherVersions>
			<!--
			for otherVersions, you can put either unsupported or unconfirmed
			-->
		</firmware>
	</compatibility>
	<dependencies>
        <package>mobilesubstrate</package>
    </dependencies>
	<descriptionlist>
        <description>Compatible with iOS 11 to 13</description>
        <description><![CDATA[
					iksafari safari features!<br/><br/>
		]]>
		</description>
	</descriptionlist>
	<screenshots>
        <screenshot>
            <description>Tweak in action</description>
            <image>1.PNG</image>
        </screenshot>
        <screenshot>
            <description>Settings for the tweak.</description>
            <image>2.PNG</image>
        </screenshot>
    </screenshots>
	<changelog>
        <change><![CDATA[
            <strong>v1.5~beta2</strong> <br/>
            -Fixed some bugs. <br/>
            <strong>v1.5~beta1</strong> <br/>
            -Completely <strong>Rewritten for iOS 12 and earlier</strong><br/>
            -Features are the same, but the implementation is much cleaner!]]>
        </change>
	</changelog>
	<links></links>
</package>
