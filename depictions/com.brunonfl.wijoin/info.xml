<package>
	<id>com.brunonfl.wijoin</id>
	<name>WiJoin</name>
	<version>1.1-58</version>
	<compatibility>
		<firmware>
            <miniOS>7.0</miniOS>
			<maxiOS>12.5</maxiOS>
			<otherVersions>unconfirmed</otherVersions>
			<!--
			for otherVersions, you can put either unsupported or unconfirmed
			-->
		</firmware>
	</compatibility>
	<dependencies>
        <package>mobilesubstrate</package>
    </dependencies>
	<descriptionlist>
        <description>Compatible from iOS 7 to 12</description>
        <description><![CDATA[
					Simple tweak to display the Wi-Fi network you connected to like in Android.
					Short Hold the message to open to Wi-Fi settings!<br/><br/>
					Configure options in settings.
					]]>
		</description>
	</descriptionlist>
	<screenshots>
        <screenshot>
            <description>Tweak in action.</description>
            <image>1.PNG</image>
        </screenshot>
        <screenshot>
            <description>Tweak in action.</description>
            <image>2.PNG</image>
        </screenshot>
    </screenshots>
	<changelog>
    <change><![CDATA[
			- Added the ability to change the height of the alert.<strong>(Experimental)</strong>]]></change>
	</changelog>
	<links></links>
</package>
