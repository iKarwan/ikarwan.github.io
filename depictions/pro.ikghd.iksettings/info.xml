<package>
	<id>pro.ikghd.iksettings</id>
	<name>iKSettings</name>
	<version>3.0-5</version>
	<compatibility>
		<firmware>
            <miniOS>11.0</miniOS>
			<maxiOS>13.7</maxiOS>
			<otherVersions>unconfirmed</otherVersions>
			<!--
			for otherVersions, you can put either unsupported or unconfirmed
			-->
		</firmware>
	</compatibility>

	<!-- main description -->
	<descriptionlist>
        <description>Compatible with iOS 11 to 13.7</description>
        <description><![CDATA[
					iKSettings included features! <br/><br/>

					--General--<br/>
					-set custom settings text<br/>
					-set custom appl id name<br/>
					-set custom icon table name<br/>
					-Rounded Settings<br/>
					-Remove Black Line<br/>
					-Remove iCon Table<br/>
					-Remove Setting Label<br/>
					-Remove TOP Settings TxT<br/>
					-Disable SearchBar<br/>
					-Remove ThirdPartyApps<br/>

			]]>

		</description>
	</descriptionlist>

